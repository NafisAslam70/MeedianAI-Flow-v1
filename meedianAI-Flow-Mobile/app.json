{
  "expo": {
    "name": "MeedianAI Flow",
    "slug": "meedianai-flow-mobile",
    "scheme": "meedianai-flow",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": { "supportsTablet": true, "bundleIdentifier": "com.meedian.ai.flow" },
    "android": { "package": "com.meedian.ai.flow" },
    "plugins": [
      "expo-router"
    ]
  }
}
